ount() {}, //生命周期 - 挂载之前\r\n  beforeUpdate() {}, //生命周期 - 更新之前\r\n  updated() {}, //生命周期 - 更新之后\r\n  beforeDestroy() {}, //生命周期 - 销毁之前\r\n  destroyed() {}, //生命周期 - 销毁完成\r\n  activated() {} //如果页面有keep-alive缓存功能，这个函数会触发\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.music-header {\r\n  display: flex;\r\n  height: 60px;\r\n  justify-content: space-between;\r\n  padding: 5px;\r\n  align-items: center;\r\n  .music-header-left {\r\n    color: white;\r\n  }\r\n  .music-header-right {\r\n    input {\r\n      width: 300px;\r\n      height: 30px;\r\n      border: 0px;\r\n      border-radius: 20px;\r\n      text-indent: 15px;\r\n      background: url(/imgs/zoom.png) 270px no-repeat white;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}